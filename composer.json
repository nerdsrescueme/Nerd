{
    "name": "nerdsrescueme/nerd",
    "description": "Nerd Content Management System",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Frank Bardon Jr.",
            "email": "nerdsrescueme@gamil.com"
        }
    ],
    "require": {
        "doctrine/orm": "2.4.x-dev",
        "twig/twig": "1.12.*",
        "doctrine/migrations": "dev-master",
        "gedmo/doctrine-extensions": "2.3.3",
        "monolog/monolog": "1.3.*",
        "symfony/http-foundation": "2.1.*",
        "swiftmailer/swiftmailer": "4.2.*",
        "symfony/validator": "2.1.*",
        "pimple/pimple": "v1.0.1",
        "frankbardon/secs": "*",
        "symfony/event-dispatcher": "2.2.*",
        "aura/router": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phing/phing": "2.4.*",
        "doctrine/data-fixtures": "1.0.x-dev"
    },
    "autoload": {
        "psr-0": {
            "CMS\\": "src/",
            "Twig_": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.med.upenn.edu:frankbardon/secs.git"
        }
    ]
}
